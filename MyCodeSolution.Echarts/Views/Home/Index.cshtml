                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6">
                    <div class="chart">
                        <a target="_blank" class="chart-link" href="/pie/pie2/">
                            <h4 class="chart-title">环形图2</h4>
                            <img class="chart-area" src="~/Content/Images/pie2.jpg">
                        </a>
                    </div>
                </div>
                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6">
                    <div class="chart">
                        <a target="_blank" class="chart-link" href="/pie/pie3/">
                            <h4 class="chart-title">环形图3</h4>
                            <img class="chart-area" src="~/Content/Images/pie3.jpg">
                        </a>
                    </div>
                </div>
                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6">
                    <div class="chart">
                        <a target="_blank" class="chart-link" href="/pie/pie4/">
                            <h4 class="chart-title">环形图4</h4>
                            <img class="chart-area" src="~/Content/Images/pie4.jpg">
                        </a>
                    </div>
                </div>
            </div>
            <h3 class="chart-type-head" id="chart-type-pid">雷达图<span>Radar</span></h3>
            <div class="row" id="chart-row-pie">
                <div class="col-xl-2 col-lg-3 col-md-4 col-sm-6">
                    <div class="chart">
                        <a target="_blank" class="chart-link" href="/radar/radar1/">
                            <h4 class="chart-title">基础雷达图</h4>
                            <img class="chart-area" src="~/Content/Images/radar1.jpg">
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Content/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Content/Scripts/bootstrap.js"></script>
